    "# Find the top 10 major project themes (using column 'mjtheme_namecode')\n",
    "mj = world_bank['mjtheme_namecode']\n",
    "\n",
    "# Initiate an empty dataframe, and then use for loop and json_normalize to create and flatten data\n",
    "mj_df = pd.DataFrame(columns = ['code', 'name'])\n",
    "\n",
    "for x in mj:\n",
    "    mj_df = mj_df.append(json_normalize(x))\n",
    "\n",
    "mj_df.head(10)"
   ]
