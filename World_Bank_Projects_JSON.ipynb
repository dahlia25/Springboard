   "source": [
    "# In the dataframe above, some entries have only the code and the name is missing. \n",
    "# Create a dataframe with the missing names filled in.\n",
    "\n",
    "# Remove all missing values (in this case is \"\"), and duplicate rows\n",
    "goodmj_df = mj_df[mj_df.name !=\"\"]\n",
    "goodmj = goodmj_df.drop_duplicates()\n",
    "\n",
    "# Create a dictionary where 'code' is the key, and 'name' is the value\n",
    "mj_dict = goodmj.set_index('code').to_dict()['name']\n",
    "mj_dict\n",
    "\n",
    "# To enable the use of .replace function, read and create the json file again\n",
    "complete_mj = pd.DataFrame(columns = ['code', 'name'])\n",
    "\n",
    "for x in mj:\n",
    "    complete_mj = complete_mj.append(json_normalize(x))\n",
    "    \n",
    "complete_mj['name'] = complete_mj['code']\n",
    "\n",
    "cmj = complete_mj.replace({'name':mj_dict})\n",
    "\n",
    "# All \"\" values in the 'name' column has been filled\n",
    "cmj.head(10)"
   ]
  }
